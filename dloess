-      do 5 i=1,nv
-         write(k,61)i,vval(0,i)
-61       format('      data vval(0,',i5,') /',1pe15.6,'/')
-         do 6 j=1,d
-            write(k,62)i,j,v(i,j)
-62          format('      data v(',i5,',',i2,') /',1pe15.6,'/')
