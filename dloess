-                  rw(i)=(1-(rw(i)/cmad)**2)**2
-               else
-                  rw(i)=1
-               end if
-            end if
-    6    continue
-      end if
-      return
-      end
-      subroutine lowesp(n,y,yhat,pwgts,rwgts,pi,ytilde)
-      integer identi,execnt,i2,i3,i5,m,n
-      integer pi(n)
-      double precision c,i1,i4,mad
