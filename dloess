-     +1,p),c(1,lo(p)),c(1,hi(p)))
-         end if
-         p=p+1
-         l=lo(p)
-         u=hi(p)
-         goto 3
-c     bottom of while loop
-    4 return
-      end
-      subroutine ehg129(l,u,d,x,pi,n,sigma)
-      integer d,execnt,i,k,l,n,u
-      integer pi(n)
-      DOUBLE PRECISION machin,alpha,beta,t
-      DOUBLE PRECISION sigma(d),x(n,d)
-      DOUBLE PRECISION D1MACH
-      external D1MACH
